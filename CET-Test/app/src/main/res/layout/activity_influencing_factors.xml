<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:columnCount="1"
    android:rowCount="14"
    tools:context="com.example.oliverfries.cet_test.InfluencingFactorsActivity">

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="1">
        <TextView
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Actual Factor Set"
            android:id="@+id/actualFactorSetTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="2dp"
            android:textSize="10pt"
            android:textAlignment="center"
            android:layout_gravity="start|end|fill_horizontal"/>
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Integration"
            android:id="@+id/integrationFactorTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12"
            android:layout_marginLeft="5dp" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/integrationFactorValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1"
            android:clickable="true"
            android:onClick="onClickIntegrationFactor"/>
        <ImageView
            android:src="@drawable/ic_action_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:onClick="onClickIntegrationFactorHelp"/>
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Local Data/Processing"
            android:id="@+id/localDataFactorTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12"
            android:layout_marginLeft="5dp" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/localDataFactorValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1"
            android:clickable="true"
            android:onClick="onClickLocalDataFactor"/>
        <ImageView
            android:src="@drawable/ic_action_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3"
        android:layout_marginLeft="5dp">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Transcation Rate"
            android:id="@+id/transactionRateTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/transactionRateValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1"
            android:clickable="true"
            android:onClick="onClickTransactionRateFactor"/>
        <ImageView
            android:src="@drawable/ic_action_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </GridLayout>

    <View
        android:id="@+id/horizontal_line_influencing_factors"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:background="#000000"
        android:layout_below="@+id/project_name"/>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="1"
        android:layout_marginLeft="5dp">
        <TextView
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Processing Logic"
            android:id="@+id/processingLogicTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="2dp"
            android:textAlignment="center"
            android:layout_gravity="start|end|fill_horizontal"/>
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3"
        android:layout_marginLeft="5dp">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Arithmetic Operation"
            android:id="@+id/arithmeticOperationTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12"/>
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/arithmeticOperationValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1"
            android:clickable="true"
            android:onClick="onClickArithmeticOperationFactor" />
        <ImageView
            android:src="@drawable/ic_action_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Control Procedure"
            android:id="@+id/controlProcedureTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12"
            android:layout_marginLeft="5dp" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/controlProcedureValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1"
            android:clickable="true"
            android:onClick="onClickControlProcedureFactor" />
        <ImageView
            android:src="@drawable/ic_action_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3"
        android:layout_marginLeft="5dp">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Exception Regulation"
            android:id="@+id/exceptionRegulationTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/exceptionRegulationValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1"
            android:clickable="true"
            android:onClick="onClickExceptionRegulationFactor"/>
        <ImageView
            android:src="@drawable/ic_action_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Logic"
            android:id="@+id/logicTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12"
            android:layout_marginLeft="5dp" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/logicValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1"
            android:clickable="true"
            android:onClick="onClickLogicFactor"/>
        <ImageView
            android:src="@drawable/ic_action_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </GridLayout>

    <View
        android:id="@+id/horizontal_line_influencing_factors_reuse"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:background="#000000"
        android:layout_below="@+id/project_name" />

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Reusability"
            android:id="@+id/reusabilityTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12"
            android:layout_marginLeft="5dp" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/reusabilityValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1"
            android:clickable="true"
            android:onClick="onClickReusabilityFactor"/>
        <ImageView
            android:src="@drawable/ic_action_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Stock Conversions"
            android:id="@+id/stockConversionTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12"
            android:layout_marginLeft="5dp" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/stockConversionValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1"
            android:clickable="true"
            android:onClick="onClickStockConversionsFactor" />
        <ImageView
            android:src="@drawable/ic_action_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3"
        android:layout_marginLeft="5dp">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Adaptability"
            android:id="@+id/adaptabilityMethodTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/adaptabilityValueMethodTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1"
            android:clickable="true"
            android:onClick="onClickAdaptabilityFactor"/>
        <ImageView
            android:src="@drawable/ic_action_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </GridLayout>

    <View
        android:id="@+id/horizontal_line_influencing_factors_summary"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:background="#000000"
        android:layout_below="@+id/project_name" />

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="1">
        <TextView
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Summation"
            android:id="@+id/summationTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="2dp"
            android:textSize="10pt"
            android:textAlignment="center"
            android:layout_gravity="start|end|fill_horizontal"/>
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Sum Of Influences"
            android:id="@+id/sumOfInfluencesTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12"
            android:layout_marginLeft="5dp" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/sumOfInfluencesValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1" />
    </GridLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnCount="3">
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Factor Influence Rating"
            android:id="@+id/factorInfluenceRatingTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="12"
            android:layout_marginLeft="5dp" />
        <TextView
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0"
            android:id="@+id/factorInfluenceRatingValueTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minEms="1" />
    </GridLayout>

</GridLayout>
